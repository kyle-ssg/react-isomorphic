{
  "name": "react-isomorphic",
  "version": "0.0.1",
  "description": "Isomorphic React boilerplate with React Router, Webpack, Express and Handlebars",
  "repository": {
    "git": "https://github.com/kyle-ssg/react-isomorphic"
  },
  "license": "MIT",
  "author": "kyle-ssg",
  "scripts": {
    "webpack:prod": "webpack --config ./config/webpack.config.prod.js",
    "server": "babel-node server --presets=es2015-node5,stage-2,react",
    "start": "nodemon --watch server --watch config --exec npm run server",
    "prod": "npm run webpack:prod && NODE_ENV=production babel-node server --presets=es2015-node5,stage-2,react"
  },
  "dependencies": {
    "bootstrap": "4.0.0-alpha.3",
    "compass-mixins": "0.12.10",
    "react": "15.3.1",
    "react-document-title": "2.0.2",
    "react-dom": "15.3.1",
    "react-router": "2.8.1"
  },
  "devDependencies": {
    "babel-cli": "6.14.0",
    "babel-core": "6.11.4",
    "babel-eslint": "6.1.2",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.14.0",
    "babel-preset-es2015-node5": "^1.2.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.11.0",
    "clean-webpack-plugin": "0.1.10",
    "copy-webpack-plugin": "3.0.1",
    "css-loader": "0.23.1",
    "express": "4.14.0",
    "express-handlebars": "3.0.0",
    "extract-text-webpack-plugin": "1.0.1",
    "html-loader": "0.4.3",
    "html-webpack-plugin": "2.22.0",
    "image-webpack-loader": "2.0.0",
    "ngrok": "2.2.2",
    "node-sass": "3.8.0",
    "postcss-loader": "0.9.1",
    "react-hot-loader": "1.3.0",
    "sass-loader": "4.0.0",
    "style-loader": "0.13.1",
    "webpack": "1.13.1",
    "webpack-dashboard": "https://github.com/kyle-ssg/webpack-dashboard.git#master",
    "webpack-dev-middleware": "1.7.0",
    "webpack-dev-server": "1.14.1",
    "webpack-hot-middleware": "2.12.2"
  },
  "keywords": [
    "react",
    "isomorphic",
    "webapack",
    "hmr",
    "boilerplate"
  ]
}
